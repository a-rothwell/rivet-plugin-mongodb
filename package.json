{
  "name": "rivet-plugin-mongodb",
  "version": "0.0.3",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "build": "tsc -b && tsx bundle.ts",
    "dev": "run-p watch serve",
    "watch": "run-p watch:tsc watch:esbuild",
    "watch:tsc": "tsc -b -w",
    "watch:esbuild": "tsx bundle.ts --watch",
    "serve": "serve --config ../serve.json dist"
  },
  "dependencies": {
    "@ironclad/rivet-core": "^1.14.0",
    "mongodb": "^6.3.0"
  },
  "devDependencies": {
    "esbuild": "^0.19.2",
    "npm-run-all": "^4.1.5",
    "serve": "^14.2.1",
    "tsx": "^3.12.10",
    "typescript": "^5.2.2"
  },
  "volta": {
    "node": "20.6.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/a-rothwell/rivet-plugin-mongodb.git"
  }
}
